{
  "name": "@mapcat/mapview-init",
  "version": "0.0.9",
  "description": "The @mapcat/mapview-init javascript library is used to obtain MAPCAT Visualization API.",
  "main": "./src/mapcat-mapview-init.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "browserify --debug ./src/mapcat-mapview-init.js --standalone mapcatview > dist/mapcatview-dev.js",
    "build-min": "browserify ./src/mapcat-mapview-init.js -t uglifyify --standalone mapcatview | uglifyjs > dist/mapcatview-min.js",
    "lint": "./node_modules/.bin/eslint src/mapcat-mapview-init.js"
  },
  "dependencies": {
    "browserify": "^14.5.0",
    "uglify-js": "^2.6.0",
    "uglifyify": "^4.0.0"
  },
  "repository": "git+https://github.com/MAPCATcom/mapcat-mapview-init.git",
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MAPCATcom/mapcat-mapview-init/issues"
  },
  "homepage": "https://github.com/MAPCATcom/mapcat-mapview-init#readme",
  "devDependencies": {
    "eslint": "https://registry.npmjs.org/eslint/-/eslint-3.19.0.tgz",
    "eslint-plugin-es5": "^1.1.0"
  },
  "standard": {
    "ignore": [
      "dist/"
    ]
  }
}
